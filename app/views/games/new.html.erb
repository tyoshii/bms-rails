<h1>Games#new</h1>
<p>Find me in app/views/games/new.html.erb</p>

<%= form_for(@game) do |f| %>
  <div class="field">
    <%= f.label :date %>
    <%= f.date_field :date %>
  </div>
  <div class="field">
    <%= f.label :start_time %>
    <%= f.time_field :start_time %>
  </div>
  <div class="field">
    <%= f.label :stadium %>
    <%= f.text_field :stadium %>
  </div>
  <div class="field">
    <%= f.label :memo %>
    <%= f.text_area :memo %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
